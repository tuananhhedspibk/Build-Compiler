Program Fibo;
var n : integer;
    i : integer;
Function Fibonaci(n : integer) : Integer;
Begin
    if n = 0 then return Fibonaci := 0;
    if n = 1 then return Fibonaci := 1;
    if n > 1 then return Fibonaci := Fibonaci(n - 1) + Fibonaci(n - 2);
End;

Begin
   n := readi;
   i := Fibonaci(n);
   call writei(i);
End.
